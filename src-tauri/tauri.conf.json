{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "AI Transcribe Local",
  "version": "0.1.0",
  "identifier": "com.niberium.ai_transcribe_local",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "AI Transcribe Local",
        "width": 1520,
        "height": 900,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": false,
    "targets": "all"
  }
}
